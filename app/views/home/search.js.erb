deleteOverlays();
$('#yogalist').html('');

<% @yogas.each_with_index do |yoga, i| %>

    <%if i < 10%>
    <%#binding.pry%>

    $('#yogalist').append('<%= j(render 'yoga', :yoga => yoga) %>');
    <%#binding.pry%>
    <% end %>

    <%@yogas.each do |item| %>
    <%#binding.pry%>
        addMarker(<%= item.latitude %>,<%= item.longitude %>,'<%= item.name%>');
    <%#binding.pry%>
     <% end %>
 <% end %>



